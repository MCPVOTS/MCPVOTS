{
  "name": "mcpvots",
  "version": "2.0.0",
  "description": "Next-generation MCP Integration with High Contrast Dark Theme",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --apply .",
    "lint:fix": "biome check --apply --unsafe .",
    "format": "biome format --write .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:wcag": "pa11y http://localhost:3000",
    "test:mcp-integration": "jest --testPathPattern=mcp",
    "test:websocket": "jest --testPathPattern=websocket",
    "test:mcp-protocol": "jest --testPathPattern=protocol",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "start:mcp-test-server": "node test-server/mcp-test-server.js",
    "ecosystem:build": "python advanced_orchestrator.py --mode build",
    "ecosystem:run": "python advanced_orchestrator.py --mode run --auto-start",
    "ecosystem:monitor": "python advanced_orchestrator.py --mode monitor",
    "ecosystem:full": "python advanced_orchestrator.py --auto-start",
    "services:start": "python ecosystem_builder.py",
    "services:health": "python -c \"import requests; print(requests.get('http://localhost:8091/health').json())\"",
    "dashboard:open": "start http://localhost:3000",
    "logs:view": "tail -f orchestrator.log",
    "backup:create": "python -c \"from advanced_orchestrator import MCPVotsOrchestrator; import asyncio; asyncio.run(MCPVotsOrchestrator().create_backup())\"",
    "metrics:export": "python -c \"print('Metrics export functionality')\"",
    "demo:quick": "npm run ecosystem:build && npm run ecosystem:run"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "dark-theme",
    "dashboard",
    "integration",
    "javascript",
    "web-application",
    "theme-manager",
    "real-time",
    "monitoring"
  ],
  "author": "MCPVots Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kabrony/MCPVots.git"
  },
  "bugs": {
    "url": "https://github.com/kabrony/MCPVots/issues"
  },
  "homepage": "https://github.com/kabrony/MCPVots#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pa11y": "^9.0.0",
    "prettier": "^3.0.0",
    "serve": "^14.0.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.0",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.0",
    "@tailwindcss/typography": "^0.5.0",
    "class-variance-authority": "^0.7.0",
    "@tanstack/react-query": "^5.0.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwindcss": "^3.4.0",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}

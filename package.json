
{
  "name": "mcpvots",
  "version": "1.0.0",
  "description": "Advanced Dark Theme MCP Integration Platform",
  "main": "src/app.js",
  "scripts": {
    "start": "serve .",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:wcag": "pa11y http://localhost:3000",
    "test:mcp-integration": "jest --testPathPattern=mcp",
    "test:websocket": "jest --testPathPattern=websocket",
    "test:mcp-protocol": "jest --testPathPattern=protocol",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/ css/",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "start:mcp-test-server": "node test-server/mcp-test-server.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "dark-theme",
    "dashboard",
    "integration",
    "javascript",
    "web-application",
    "theme-manager",
    "real-time",
    "monitoring"
  ],
  "author": "MCPVots Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kabrony/MCPVots.git"
  },
  "bugs": {
    "url": "https://github.com/kabrony/MCPVots/issues"
  },
  "homepage": "https://github.com/kabrony/MCPVots#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pa11y": "^7.0.0",
    "prettier": "^2.8.0",
    "serve": "^14.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "dependencies": {
    "ws": "^8.14.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}

{
  "name": "mcpvots",
  "version": "2.0.0",
  "description": "Next-generation MCP Integration with High Contrast Dark Theme",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --apply .",
    "lint:fix": "biome check --apply --unsafe .",
    "format": "biome format --write .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:a11y": "jest --testPathPattern=accessibility",
    "test:wcag": "pa11y http://localhost:3000",
    "test:mcp-integration": "jest --testPathPattern=mcp",
    "test:websocket": "jest --testPathPattern=websocket",
    "test:mcp-protocol": "jest --testPathPattern=protocol",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "start:mcp-test-server": "node test-server/mcp-test-server.js",
    "ecosystem:build": "python advanced_orchestrator.py --mode build",
    "ecosystem:run": "python advanced_orchestrator.py --mode run --auto-start",
    "ecosystem:monitor": "python advanced_orchestrator.py --mode monitor",
    "ecosystem:full": "python advanced_orchestrator.py --auto-start",
    "n8n:setup": "python n8n_agi_launcher.py --mode full",
    "n8n:quick": "python n8n_agi_launcher.py --mode quick",
    "n8n:shutdown": "python n8n_agi_launcher.py --mode shutdown",
    "n8n:test": "python test_n8n_integration.py",
    "n8n:dashboard": "start http://localhost:5678",
    "services:start": "python ecosystem_builder.py",
    "services:health": "python -c \"import requests; print(requests.get('http://localhost:8091/health').json())\"",
    "dashboard:open": "start http://localhost:3000",
    "logs:view": "tail -f orchestrator.log",
    "backup:create": "python -c \"from advanced_orchestrator import MCPVotsOrchestrator; import asyncio; asyncio.run(MCPVotsOrchestrator().create_backup())\"",
    "metrics:export": "python -c \"print('Metrics export functionality')\"",
    "demo:quick": "npm run ecosystem:build && npm run ecosystem:run"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "dark-theme",
    "dashboard",
    "integration",
    "javascript",
    "web-application",
    "theme-manager",
    "real-time",
    "monitoring"
  ],
  "author": "MCPVots Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kabrony/MCPVots.git"
  },
  "bugs": {
    "url": "https://github.com/kabrony/MCPVots/issues"
  },
  "homepage": "https://github.com/kabrony/MCPVots#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pa11y": "^9.0.0",
    "prettier": "^3.0.0",
    "serve": "^14.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.0",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-three/drei": "^10.3.0",
    "@react-three/fiber": "^9.1.2",
    "@tailwindcss/typography": "^0.5.0",
    "@tanstack/react-query": "^5.81.2",
    "@types/d3": "^7.4.3",
    "@types/three": "^0.177.0",
    "@voltagent/core": "^0.1.46",
    "@voltagent/vercel-ai": "^0.1.13",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "framer-motion": "^12.19.1",
    "lucide-react": "^0.294.0",
    "n8n": "^1.99.1",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.0.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "three": "^0.177.0",
    "ws": "^8.18.2",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
